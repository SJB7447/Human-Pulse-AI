{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/**/*": {
            "maxDuration": 30
        }
    },
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 9 * * *"
        }
    ]
}
